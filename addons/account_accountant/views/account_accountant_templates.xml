<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_backend" name="account assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/account_accountant/static/src/less/account_dashboard.less"/>
            <script type="text/javascript" src="/account_accountant/static/src/js/account_dashboard.js"></script>
        </xpath>
    </template>

    <template id="qunit_suite" name="account_accountant_tests" inherit_id="web.qunit_suite">
        <xpath expr="//t[@t-set='head']" position="inside">
            <script type="text/javascript" src="/account_accountant/static/tests/account_accountant_dashboard_tests.js"></script>
        </xpath>
    </template>
</odoo>
