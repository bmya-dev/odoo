            <group>
                <filter string="AFIP Responsability Type" domain="[]" name="l10n_ar_afip_responsability_type_filter" context="{'group_by':'l10n_ar_afip_responsability_type'}"/>
            </group>
        </field>
    </record>
</odoo>
